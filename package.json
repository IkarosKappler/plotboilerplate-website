    "devDependencies": {}
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@mxssfd/typedoc-theme": "^1.1.3",
    "postcss-cli": "^11.0.0",
    "sass": "^1.75.0",
    "typedoc": "^0.25.13",
    "typedoc-material-theme": "^1.0.2",
